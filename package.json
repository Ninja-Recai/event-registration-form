{
  "name": "register-form",
  "version": "1.0.0",
  "description": "Event registration form",
  "main": "app.js",
  "private": true,
  "author": "Recai",
  "license": "ISC",
  "engines": {
    "node": "9.5.0"
  },
  "scripts": {
    "start": "concurrently --raw \"npm run server\" \"npm run client --color\"",
    "test": "concurrently --raw \"npm run test:server --color\" \"npm run test:client --color\"",
    "server": "node ./bin/www",
    "client": "node start-client.js",
    "build:client": "node build-client.js",
    "test:client": "node test-client.js",
    "test:server": "jest --detectOpenHandles",
    "debug": "node-debug ./bin/www",
    "express": "node ./bin/www",
    "express-debug": "node-debug ./bin/www"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "debug": "^3.1.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-validator": "^5.3.0",
    "mongodb": "^3.1.4",
    "mongoose": "^5.1.0",
    "morgan": "^1.9.0",
    "pug": "^2.0.3"
  },
  "devDependencies": {
    "concurrently": "^3.1.0",
    "jest": "^23.6.0",
    "supertest": "^3.3.0"
  },
  "jest": {
    "testRegex": "tests/.*\\.test.js$"
  }
}
